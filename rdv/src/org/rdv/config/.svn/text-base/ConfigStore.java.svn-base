
package org.rdv.config;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory;
import org.rdv.ConfigurationManager;

/**
 * Template for saving and loading configurations
 */
abstract public class ConfigStore {
  static Log log = LogFactory.getLog(ConfigurationManager.class.getName());
  public abstract void saveConfiguration(String configName);
  public abstract void loadConfiguration(String configName);
}
